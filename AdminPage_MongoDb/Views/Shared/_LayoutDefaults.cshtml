<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="robots" content="noindex">
    <meta name="googlebot" content="noindex">
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="manifest" href="~/manifest.json">
    <title>@ViewData["Title"] - AdminPage_MongoDB</title>
    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <!-- CSS Files -->
    <link href="https://unpkg.com/gijgo@1.9.11/css/gijgo.min.css" rel="stylesheet" type="text/css" />
    @*<link href="~/assets/css/material-dashboard.css" rel="stylesheet" />*@
    <link href="https://cdn.jsdelivr.net/npm/material-dashboard@2.1.0/assets/css/material-dashboard.min.css" rel="stylesheet" />
    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="~/assets/demo/demo.css" rel="stylesheet" />
    @RenderSection("Link", required: false)
</head>
<body class="dark-edition">
    <div class="wrapper">
        @await Html.PartialAsync("_SideBar")
        <div class="main-panel">
            <partial name="_NavBar" />
            <div class="content">
                <div class="container-fluid">
                    <div id="discussion"></div>
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <script src="~/assets/js/core/jquery.min.js" asp-append-version="true"></script>
    <script src="~/assets/js/core/popper.min.js" asp-append-version="true"></script>
    @*<script src="https://unpkg.com/gijgo@1.9.11/js/gijgo.min.js" type="text/javascript"></script>*@
    <script src="~/assets/js/core/bootstrap-material-design.min.js" asp-append-version="true"></script>
    <script src="https://unpkg.com/default-passive-events" asp-append-version="true"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.perfect-scrollbar/1.4.0/perfect-scrollbar.min.js"></script>*@
    <script src="~/assets/js/plugins/perfect-scrollbar.jquery.min.js" asp-append-version="true"></script>
    <!-- Place this tag in your head or just before your close body tag. -->
    <script async defer src="https://cdnjs.cloudflare.com/ajax/libs/Buttons/2.0.0/js/buttons.min.js" asp-append-version="true"></script>
    @*<script async defer src="https://buttons.github.io/buttons.js"></script>*@
    <!-- Chartist JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/chartist/0.11.0/chartist.min.js" asp-append-version="true"></script>
    @*<script src="~/assets/js/plugins/chartist.min.js"></script>*@
    <!--  Notifications Plugin    -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-notify/0.2.0/js/bootstrap-notify.min.js" asp-append-version="true"></script>
    @*<script src="~/assets/js/plugins/bootstrap-notify.js"></script>*@
    <!-- Control Center for Material Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="https://cdn.jsdelivr.net/npm/material-dashboard@2.1.0/assets/js/material-dashboard.min.js" asp-append-version="true"></script>
    @*<script src="~/assets/js/material-dashboard.js?v=2.1.0"></script>*@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/gijgo/1.9.11/combined/js/gijgo.min.js" asp-append-version="true"></script>*@
    <script src="~/Lib/signalR/jquery.signalR-2.4.0.min.js" asp-append-version="true"></script>
    <script src="~/hubs" asp-append-version="true"></script>
    <script src="~/Contents/js/DesignTemplate.js" asp-append-version="true"></script>
    <script src="~/Contents/js/customer.js" asp-append-version="true"></script>
    <script>
        $('.datepicker').datetimepicker({
          format: 'MM/DD/YYYY',
          icons: {
            time: "fa fa-clock-o",
            date: "fa fa-calendar",
            up: "fa fa-chevron-up",
            down: "fa fa-chevron-down",
            previous: 'fa fa-chevron-left',
            next: 'fa fa-chevron-right',
            today: 'fa fa-screenshot',
            clear: 'fa fa-trash',
            close: 'fa fa-remove'
          }
        });
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
