@{
    Layout = "~/Views/Shared/_LayoutStudent.cshtml";
}

<div id="schedule"></div>


@section Scripts{ 
    <script src="~/js/lib/schedule.js"></script>
    <script>
        const schedule = CalendarLib.Schedule();
        schedule.onLoad({
            type: "@User?.FindFirst("Type").Value",
            controller: 'Calendar'
        });
    </script>
}